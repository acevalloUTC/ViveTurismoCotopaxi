<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.BlankFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="124dp">

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:weightSum="1">

        <TextView
            android:text="CALENDARIO DE ACONTECIMIENTOS"
            android:layout_width="440dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="95dp">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="77dp">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:text="Fiestas del Señor de la Buena Esperanza- Parroquializacion"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView5" />

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            <TextView
                                android:text="Plaza grande"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/textView22" />
                        </TableRow>

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:text="Fiesta de la Virgen de la Victoria"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView7"
                            android:textSize="14sp"
                            android:textAlignment="textStart" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:text="Fiesta de San Agustin"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView8"
                            android:lineSpacingExtra="10sp" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:text="Finados"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView12" />

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView15"

                        </TableRow>

                    </TableRow>
                </TableLayout>

                <TextView
                    android:text="Fiesta de Niño de Isinche"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:text="Fiesta de los Caporales"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView21" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"></LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView23"
                            android:text="CANTON SALCEDO"
                            android:maxWidth="@dimen/fab_margin" />
                    </LinearLayout>
                </TableRow>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</FrameLayout>
