<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"

    android:paddingTop="@dimen/activity_vertical_margin"

    tools:context="com.example.kenshi.informacion.Informacion$PlaceholderFragment"
    android:background="@mipmap/inib">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="388dp"
            android:layout_height="46dp"></LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="290dp"
            android:layout_height="53dp">

            <TextView
                android:text="Poblacion."
                android:layout_width="122dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                android:textColor="@android:color/black"
                android:textSize="18sp"
                android:textColorHighlight="@android:color/background_dark"
                android:fontFamily="sans-serif"
                android:layout_row="0"
                android:layout_column="0" />

            <TextView
                android:text="409.205 habitantes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:textColor="@android:color/white"
                android:layout_alignTop="@+id/textView14"
                android:layout_toEndOf="@+id/textView14" />
        </LinearLayout>
    </FrameLayout>

    <TextView
        android:text="Extension Territorial."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:id="@+id/textView14"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <TextView
        android:text="6.109 km2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:textSize="18sp"
        android:textColor="@android:color/white"
        android:layout_alignTop="@+id/textView14"
        android:layout_toEndOf="@+id/textView14" />

    <TextView
        android:text="Rango Altitudinal."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/textView17"
        android:layout_below="@+id/textView14"
        android:textSize="18sp"
        android:textColor="@android:color/black" />



    <TextView
        android:text="Varian desde los 90 msnm en la parroquia Moraspungo del Cantón Pangua, asciendo hasta  4.480 msnm en la cordillera  Occidental en el Sector de la Comunidad de Apagua, desciende a los 2.760 ,msnm en la Ciudad de Latacunga, para subir nuevamente hasta el punto mas alto localizado en el borde del cráter del Volcan Cotopaxi con una altitud de 5.920 msnm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_below="@+id/textView17"
        android:textSize="18sp"
        android:textColor="@android:color/white"
        android:textAlignment="viewStart" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_below="@+id/textView19"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="13dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/textView17"
        android:layout_height="212dp"
        android:layout_width="match_parent"
        android:weightSum="1">

        <TextView
            android:text="Limites"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView19"
            android:layout_marginBottom="69dp"
            android:id="@+id/textView20"
            android:textSize="24sp"
            android:textColor="@android:color/holo_red_dark"
            android:layout_row="19"
            android:layout_column="1" />


    </LinearLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView19"
        android:layout_toEndOf="@+id/textView14">

        <TextView
            android:text="409,205 habitantes"
            android:layout_width="158dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:textSize="14sp"
            android:textColor="@android:color/white"
            android:layout_row="24"
            android:layout_column="0" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="189dp"
            android:layout_height="173dp"
            android:layout_row="2"
            android:layout_column="0">

        </LinearLayout>

    </GridLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="233dp"
        android:layout_height="106dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true">

        <TextView
            android:layout_width="match_parent"
            android:text="Norte: Provincia de Pichincha y Santo Domingo de los Tsachilas "
            android:textColor="@android:color/white"
            android:layout_height="wrap_content"
            android:layout_marginStart="114dp"
            android:layout_marginTop="28dp"
            android:layout_alignTop="@+id/linearLayout"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="match_parent"
            android:text="Sur: Provincia de Tungurahua y Bolivar"
            android:textColor="@android:color/white"
            android:layout_height="wrap_content"
            android:layout_weight="wrap_content" />

        <TextView
            android:layout_width="match_parent"
            android:text="Este: Provincia de Napo"
            android:textColor="@android:color/white"
            android:layout_height="wrap_content"
            android:layout_weight="wrap_content" />

        <TextView
            android:layout_width="match_parent"
            android:text="Oeste: Provincias de Santo Domingo de los Tsachilas y los Rios"
            android:textColor="@android:color/white"
            android:layout_height="wrap_content"
            android:layout_weight="wrap_content" />

    </LinearLayout>

</RelativeLayout>
