<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.LaManaSimbolos"
    style="@style/fondo"
    android:background="@drawable/fondo1">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:id="@+id/imageView13"
            android:src="@drawable/franja1" />

        <ScrollView
            style="@style/layoutSimbolos"
            android:layout_width="wrap_content"
            android:layout_height="419dp"
            android:id="@+id/scrollView5"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="0dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:layout_alignParentBottom="true" >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:weightSum="1" >

                <TextView
                    style="@style/tituloDatos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Bandera de La Mana"
                    android:id="@+id/textView14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="130dp"
                    android:id="@+id/imageView14"
                    android:src="@drawable/bandera_lamana" />

                <TextView
                    style="@style/tituloDatos"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp"
                    android:text="Escudo de La Mana"
                    android:id="@+id/textView15" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:id="@+id/imageView15"
                    android:src="@drawable/escudo_lamana" />


            </LinearLayout>
        </ScrollView>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:id="@+id/imageView17"
            android:src="@drawable/franja1" />
    </LinearLayout>

</RelativeLayout>
